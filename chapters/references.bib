	keywords = {Psychology, Geography, Environmental Effects, Behavioral Ecology, Environmental Psychology, Spatial Organization},
	file = {Snapshot:C\:\\Users\\feraco\\Zotero\\storage\\TYH8BKCY\\2013-14629-000.html:text/html},
