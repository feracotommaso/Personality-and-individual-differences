	author = {Feraco, Tommaso and Casali, Nicole and Ganzit, Elena and Meneghetti, Chiara},
	year = {2023},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/bjep.12560},
	keywords = {academic achievement, emotions, self-regulated learning, academic self-efficacy, adaptability, life satisfaction},
