	urldate = {2024-07-11},
	journal = {The Lancet Planetary Health},
	author = {Hickman, Caroline and Marks, Elizabeth and Pihkala, Panu and Clayton, Susan and Lewandowski, R Eric and Mayall, Elouise E and Wray, Britt and Mellor, Catriona and van Susteren, Lise},
	month = dec,
	year = {2021},
	keywords = {notion},
	pages = {e863--e873},
}

