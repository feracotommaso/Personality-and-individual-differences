	note = {Publisher: American Psychological Association},
	keywords = {character strengths, Personality, Humans, Psychology, Character, Cities, entrepreneurship, Entrepreneurship, presidential voting, Social Environment, U.S. cities, United States, Urban Environments, Urban Population, urban psychology, Urbanization, Voting Behavior},
	pages = {535--547},
}

@article{legree_correlations_1996,
