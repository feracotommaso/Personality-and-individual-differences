@article{sun_how_2024,
	title = {How and why people want to be more moral},
	volume = {92},
	issn = {1467-6494},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/jopy.12812},
	doi = {10.1111/jopy.12812},
	abstract = {Objective What types of moral improvements do people wish to make? Do they hope to become more good, or less bad? Do they wish to be more caring? More honest? More loyal? And why exactly do they want to become more moral? Presumably, most people want to improve their morality because this would benefit others, but is this in fact their primary motivation? Here, we begin to investigate these questions. Method Across two large, preregistered studies (N = 1818), participants provided open-ended descriptions of one change they could make in order to become more moral; they then reported their beliefs about and motives for this change. Results In both studies, people most frequently expressed desires to improve their compassion and more often framed their moral improvement goals in terms of amplifying good behaviors than curbing bad ones. The strongest predictor of moral motivation was the extent to which people believed that making the change would have positive consequences for their own well-being. Conclusions Together, these studies provide rich descriptive insights into how ordinary people want to be more moral, and show that they are particularly motivated to do so for their own sake.},
	language = {en},
	number = {3},
	urldate = {2024-06-24},
	journal = {Journal of Personality},
	author = {Sun, Jessie and Wilt, Joshua and Meindl, Peter and Watkins, Hanne M. and Goodwin, Geoffrey P.},
	year = {2024},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/jopy.12812},
	keywords = {personality change, well-being, moral character, moral improvement, moral motivation},
	pages = {907--925},
	file = {Full Text PDF:C\:\\Users\\feraco\\Zotero\\storage\\JB3QL4ZN\\Sun et al. - 2024 - How and why people want to be more moral.pdf:application/pdf;Snapshot:C\:\\Users\\feraco\\Zotero\\storage\\DF2EZTWR\\jopy.html:text/html},
}

@misc{sun_people_2023,
	title = {People {Prioritize} {Personal} {Fulfillment} {Over} {Morality} in the {Context} of {Self}-{Improvement}},
	url = {https://osf.io/gd5pw},
	doi = {10.31234/osf.io/gd5pw},
	abstract = {Most people are interested in improving themselves, but they are much less interested in becoming more moral. Why don’t people want to be more moral, and why do people prioritize improving certain aspects of themselves over others? Here, we examine two possible explanations. First, we examine whether people seek to change the traits they believe will have the most widespread positive impacts on their personality (the Personality-Enabling Hypothesis; Study 1). Second, we examine whether people seek to change the traits that will have the greatest positive impact on their well-being (the Personal Fulfillment Hypothesis; Studies 1–2). Across two preregistered studies (Ns = 252/303), we find greater evidence for the latter than the former. Over and above the desire to address perceived trait deficiencies, people primarily seek to improve the traits that they believe would particularly help them achieve their goals and increase their happiness. Moral improvements were not seen as particularly effective at doing either, and were therefore deprioritized. Finally, whereas the tendency to deprioritize moral improvements was highly generalizable across participants, beliefs about consequences for different dimensions of personal fulfillment more strongly predicted change goals to the extent that people explicitly valued these dimensions. These results point to the centrality of well-being for understanding why people want to improve themselves, clarify the perceived tradeoffs between well-being and morality, and highlight the importance of individual differences in people’s motivations for personality change.},
	language = {en-us},
	urldate = {2024-06-24},
	publisher = {OSF},
	author = {Sun, Jessie and Berman, Jonathan},
	month = jan,
	year = {2023},
	file = {Versione inviata:C\:\\Users\\feraco\\Zotero\\storage\\ARL6MSU4\\Sun e Berman - 2023 - People Prioritize Personal Fulfillment Over Morali.pdf:application/pdf},
}

@misc{gambarota_ordinal_2024,
