	author = {Raznahan, Armin and Lee, Yohan and Stidd, Reva and Long, Robert and Greenstein, Dede and Clasen, Liv and Addington, Anjene and Gogtay, Nitin and Rapoport, Judith L. and Giedd, Jay N.},
	month = sep,
	year = {2010},
	pmid = {20841422},
	note = {Publisher: National Academy of Sciences
Section: Biological Sciences},
	keywords = {development, sex differences, androgen receptor, brain, cortex},
	pages = {16988--16993},
	file = {Snapshot:C\:\\Users\\feraco\\Zotero\\storage\\U92KL6GF\\16988.html:text/html},
}

