	author = {Laborde, Sylvain and Guillén, Félix and Mosley, Emma},
	month = sep,
	year = {2016},
	keywords = {Personality, Stable differences, Trait, Type of sport},
	pages = {9--13},
}

@article{guillen_higher-order_2014,
	title = {Higher-order structure of mental toughness and the analysis of latent mean differences between athletes from 34 disciplines and non-athletes},
	volume = {60},
