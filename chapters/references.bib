	keywords = {Measurement invariance, categorical variables, lavaan, model identification, Mplus, semTools in R},
	pages = {111--130},
	file = {Full Text PDF:C\:\\Users\\feraco\\Zotero\\storage\\ZMZDJNRN\\Svetina et al. - 2020 - Multiple-Group Invariance with Categorical Outcome.pdf:application/pdf},
}

