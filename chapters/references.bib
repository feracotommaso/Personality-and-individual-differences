	journal = {Structural Equation Modeling: A Multidisciplinary Journal},
	author = {Corrêa Ferraz, Raul and Maydeu-Olivares, Alberto and Shi, Dexin},
	month = may,
	year = {2022},
	note = {Publisher: Routledge
\_eprint: https://doi.org/10.1080/10705511.2022.2053128},
