	keywords = {Character strengths, notion, signature strengths, mindfulness, strengths interventions, strengths-based practitioner, strengths-spotting, VIA classification, VIA Survey},
	file = {Full text:C\:\\Users\\feraco\\Zotero\\storage\\6P63WYCX\\Niemiec e Pearce - 2021 - The Practice of Character Strengths Unifying Defi.pdf:application/pdf},
}

