	journal = {Current Psychology},
	author = {Granziol, Umberto and Cona, Giorgia},
	month = may,
	year = {2023},
	keywords = {Mental health, Personality, Cognition, Bayesian networks, Exploratory graph analysis, Human Connectome Project, Network},
}

@article{floyd_dependability_2009,
