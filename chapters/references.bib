	note = {Publisher: Routledge
\_eprint: https://doi.org/10.1080/10705511.2017.1387859},
	keywords = {factorial invariance, subsampling method, unbalanced sample sizes},
	pages = {201--213},
	file = {Full Text PDF:C\:\\Users\\feraco\\Zotero\\storage\\52BIDEIZ\\Yoon e Lai - 2018 - Testing Factorial Invariance With Unbalanced Sampl.pdf:application/pdf},
}

