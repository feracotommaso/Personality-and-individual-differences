	author = {Diener, Ed and Kanazawa, Satoshi and Suh, Eunkook M. and Oishi, Shigehiro},
	month = aug,
	year = {2015},
	note = {Publisher: SAGE Publications Inc},
	keywords = {well-being, emotion, evolutionary psychology},
	pages = {235--256},
}

