	file = {SAGE PDF Full Text:C\:\\Users\\feraco\\Zotero\\storage\\YFP9VQWG\\Reiff et al. - 2001 - The Relation of LD and Gender with Emotional Intelligence in College Students.pdf:application/pdf},
