	month = jun,
	year = {2018},
	keywords = {Life satisfaction, Subjective well-being, Academic achievement, Meta-analysis, Academic satisfaction},
	pages = {83--94},
}

@article{rasdi_applicability_2020,
	title = {The applicability of social cognitive career theory in predicting life satisfaction of university students: {A} meta-analytic path analysis},
