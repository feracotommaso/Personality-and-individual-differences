@article{graesser_educational_2022,
	title = {Educational {Psychology} {Is} {Evolving} to {Accommodate} {Technology}, {Multiple} {Disciplines}, and {Twenty}-{First}-{Century} {Skills}},
	volume = {73},
	url = {https://doi.org/10.1146/annurev-psych-020821-113042},
	doi = {10.1146/annurev-psych-020821-113042},
	abstract = {This article covers recent research activities in educational psychology that have an interdisciplinary emphasis and that accommodate twenty-first-century skills in addition to the traditional foundations of literacy, numeracy, science, reasoning (problem-solving), and academic subject matter. We emphasize digital technologies because they are capable of tracking learning data in rich detail and reliably delivering interventions that are tailored to individual learners in particular sociocultural contexts. This is a departure from inflexible pedagogical approaches that previously have been routinely adopted in most classrooms and other contexts of instruction with no precise record of learning and instructional activities. A good design of educational technology embraces the principles of learning science, identifies the basic types of learning that are needed, implements relevant technological affordances, and accommodates feedback from different stakeholders. This article covers research in literacy, collaborative problem-solving, motivation, emotion, and science, technology, engineering, and mathematics (STEM) areas.},
	number = {1},
	urldate = {2022-04-12},
	journal = {Annual Review of Psychology},
	author = {Graesser, Arthur C. and Sabatini, John P. and Li, Haiying},
	year = {2022},
