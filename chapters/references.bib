	author = {Tourva, Anna and Spanoudis, George and Demetriou, Andreas},
	month = jan,
	year = {2016},
	keywords = {notion, Attention, Cognitive correlates, Crystallized intelligence, Fluid intelligence, Speed of processing, WM},
	pages = {136--146},
	file = {ScienceDirect Snapshot:C\:\\Users\\feraco\\Zotero\\storage\\EV57DL53\\S0160289615001671.html:text/html},
}

@article{coyle_processing_2011,
