@article{chang_is_2019,
	title = {Is personal growth initiative associated with later life satisfaction in {Chinese} college students? {A} 15‐week prospective analysis},
	volume = {22},
	issn = {1367-2223, 1467-839X},
	shorttitle = {Is personal growth initiative associated with later life satisfaction in {Chinese} college students?},
