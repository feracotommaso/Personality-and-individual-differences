@article{jovanovic_internalizing_2022,
	title = {Internalizing behaviors in adolescence: {The} unique contribution of life satisfaction beyond positive and negative affect},
	volume = {192},
	issn = {0191-8869},
	shorttitle = {Internalizing behaviors in adolescence},
	url = {https://www.sciencedirect.com/science/article/pii/S0191886922000976},
