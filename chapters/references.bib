	keywords = {income inequality, globalization, obesity, trade liberalization and foreign direct investment},
	pages = {7--21},
	file = {Snapshot:C\:\\Users\\feraco\\Zotero\\storage\\9YAGJ6U6\\09581596.2013.html:text/html},
