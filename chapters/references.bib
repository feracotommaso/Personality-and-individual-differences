@article{putwain_resilient_2013,
	title = {Resilient children are less test anxious and perform better in tests at the end of primary schooling},
	volume = {28},
	issn = {1041-6080},
	url = {https://www.sciencedirect.com/science/article/pii/S104160801300126X},
	doi = {10.1016/j.lindif.2013.09.010},
	abstract = {Resilience has been documented as a key variable in determining successful adaption to adverse circumstances and may also influence successful educational outcomes such as test performance. This paper reports on a study involving 123 pupils from 3 English primary schools, which examined the extent to which test anxiety may mediate the association between resiliency and test performance. Non-verbal ability was measured at the beginning of the school year as a control variable, and test anxiety and resilience via self-report two weeks prior to pupils taking statutory tests in English and maths at the end of primary schooling. Using multiple regression analyses to control for differences in pupils' non-verbal ability, higher resilience was found to predict lower test anxiety and higher test scores. Test anxiety, in turn, was found to predict lower test scores. The lower performance of the less resilient students was mediated by higher test anxiety. These findings indicate how resilience can account for variance in test performance and show how resilience and test anxiety may be related.},
	language = {en},
	urldate = {2021-11-19},
	journal = {Learning and Individual Differences},
	author = {Putwain, Dave W. and Nicholson, Laura J. and Connors, Liz and Woods, Kevin},
	month = dec,
	year = {2013},
	keywords = {Academic achievement, Resilience, Test anxiety, Test performance},
	pages = {41--46},
}

@article{benetti_affect-regulated_2006,
