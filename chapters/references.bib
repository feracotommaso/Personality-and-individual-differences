	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1440-172X.2005.00509.x},
	keywords = {qualitative research, subjective well-being, adolescent, family},
	pages = {125--133},
	file = {Snapshot:C\:\\Users\\feraco\\Zotero\\storage\\TIHXXJTB\\j.1440-172X.2005.00509.html:text/html},
}

@article{gonzalez-carrasco_changes_2017,
	title = {Changes with {Age} in {Subjective} {Well}-{Being} {Through} the {Adolescent} {Years}: {Differences} by {Gender}},
