	journal = {Journal of Counseling \& Development},
	author = {Weigold, Ingrid K. and Weigold, Arne and Russell, Elizabeth J. and Wolfe, Ginelle L. and Prowell, Jusiah L. and Martin-Wagar, Caitlin A.},
	year = {2020},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/jcad.12340},
	keywords = {meta-analysis, mental health, distress, personal growth initiative, wellness},
