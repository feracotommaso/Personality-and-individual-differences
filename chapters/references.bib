	url = {https://www.sciencedirect.com/science/article/pii/S0277953611002371},
	doi = {10.1016/j.socscimed.2011.03.046},
	abstract = {Richard Wilkinson’s ‘inequality hypothesis’ describes the relationship between societal income inequality and population health in terms of the corrosive psychosocial effects of social hierarchy. An explicit component of this hypothesis is that inequality should lead individuals to become more competitive and self-focused, less friendly and altruistic. Together these traits are a close conceptual match to the opposing poles of the Big Five personality factor of Agreeableness; a widely used concept in the field of personality psychology. Based on this fact, we predicted that individuals living in more economically unequal U.S. states should be lower in Agreeableness than those living in more equal states. This hypothesis was tested in both ecological and multilevel analyses in the 50 states plus Washington DC, using a large Internet sample (N = 674,885). Consistent with predictions, ecological and multilevel models both showed a negative relationship between state level inequality and Agreeableness. These relationships were not explained by differences in average income, overall state socio-demographic composition or individual socio-demographic characteristics.},
	language = {en},
	number = {12},
	urldate = {2022-06-30},
	journal = {Social Science \& Medicine},
	author = {de Vries, Robert and Gosling, Samuel and Potter, Jeff},
	month = jun,
	year = {2011},
	keywords = {Income inequality, Personality, Agreeableness, Social strategies, U.S.A},
	pages = {1978--1985},
	file = {ScienceDirect Snapshot:C\:\\Users\\feraco\\Zotero\\storage\\E3BF7FC2\\S0277953611002371.html:text/html},
}

@article{loughnan_economic_2011,
	title = {Economic {Inequality} {Is} {Linked} to {Biased} {Self}-{Perception}},
