	journal = {Education},
	author = {Heiman, Tali and Kariv, Dafna},
	year = {2004},
	note = {Publisher: Project Innovation Austin LLC},
	keywords = {COLLEGE students, HIGHER education, LEARNING disabilities, LEARNING strategies, PSYCHOLOGICAL adaptation},
	pages = {313--324},
	file = {EBSCO Full Text:C\:\\Users\\feraco\\Zotero\\storage\\7VA2W6N9\\Heiman e Kariv - 2004 - Manifestations of Learning Disabilities in University Students Implications for Coping and Adjustme.pdf:application/pdf},
