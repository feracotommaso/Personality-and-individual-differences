	year = {2010},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/acp.1588},
	pages = {812--826},
	file = {Snapshot:C\:\\Users\\feraco\\Zotero\\storage\\8TAJSH5R\\acp.html:text/html},
}

